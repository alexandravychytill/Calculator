    "@testing-library/jest-native": "^5.4.2",
    "@testing-library/react-native": "^12.0.1",
    "preset": "react-native",
    "@types/react": "~18.0.14",
    "typescript": "^4.9.4"
  },
  "private": true,
  "jest": {
    "preset": "jest-expo",
